<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="416.0" prefWidth="511.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.eftposmachine.FXMLStockEntryController">
   <children>
      <TitledPane alignment="CENTER" animated="false" prefHeight="26.0" prefWidth="559.0" text="Application Register- Enter Stock" />
      <Button fx:id="buttonAddStock" layoutX="26.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleButtonAddStockAction" prefHeight="30.0" prefWidth="164.0" text="Add Product to Order" />
      <Button fx:id="buttonSaveStock" layoutX="391.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleButtonSaveStockAction" prefHeight="30.0" prefWidth="164.0" text="Save Purchase" />
      <Button fx:id="buttonExit" layoutX="391.0" layoutY="370.0" mnemonicParsing="false" onAction="#handleButtonExitAction" prefHeight="30.0" prefWidth="164.0" text="Back" />
      <TextArea fx:id="tfStockDis" layoutX="202.0" layoutY="55.0" prefHeight="250.0" prefWidth="353.0" />
      <TextField fx:id="tfProductID" layoutX="33.0" layoutY="55.0" promptText="Product ID" />
      <TextField fx:id="tfProductQuantity" layoutX="33.0" layoutY="106.0" promptText="Product Quantity" />
      <TextField fx:id="tfProductPrice" layoutX="33.0" layoutY="158.0" promptText="Product Price" />
      <Label layoutX="33.0" layoutY="185.0" text="Only price of singular item" />
   </children>
</AnchorPane>
